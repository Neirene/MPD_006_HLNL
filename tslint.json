{
    "rules": {
        "class-name": true,
        "comment-format": [
            true
        ],
        "eofline": false,
        "indent": [
            true,
            "spaces"
        ],
        "no-duplicate-variable": true,
        "no-eval": true,
        "no-internal-module": true,
        "no-reference": false,
        "no-trailing-whitespace": true,
        "no-var-keyword": true,
        "one-line": [
            true,
            "check-open-brace",
            "check-whitespace"
        ],
        "one-variable-per-declaration": [false,
            "ignore-for-loop"
        ],
        "quotemark": [
            false,
            "single"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "trailing-comma": [true,
            {
                "singleline": "never",
                "multiline": "never"
            }
        ],
        "triple-equals": false,
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            }
        ],
        "variable-name": [
            true,
            "ban-keywords"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ]
    }
}